			},
			{
				"id": "styled-nicknames",
				"versions": ">=${styledNicknames}",
				"optional": true
			},
			{
				"id": "fabrictailor",
				"versions": ">=${fabrictailor}",
				"optional": true
			},
			{
				"id": "origins",
				"versions": ">=${origins}",
				"optional": true
			},
			{
				"id": "pehkui",
				"versions": ">=${pehkui}",
				"optional": true
			},
			{
				"id": "fabrication",
				"versions": ">=${fabrication}",
				"optional": true
			}
		]
	},
	"mixin": "switchy.mixins.json"
