{
	"schema_version": 1,
	"quilt_loader": {
		"group": "folk.sisby",
		"id": "switchy",
		"version": "${switchy}",
		"metadata": {
			"name": "Switchy",
			"description": "Commands for switching between nothing - including nicknames, skins, and more.",
			"contributors": {
				"Sisby folk": "Co-Authors"
			},
			"contact": {
				"homepage": "https://modrinth.com/mod/switchy",
				"issues": "https://github.com/sisby-folk/switchy/issues",
				"sources": "https://github.com/sisby-folk/switchy"
			},
			"icon": "assets/switchy/icon.png"
		},
		"intermediate_mappings": "net.fabricmc:intermediary",
		"entrypoints": {
			"init": "folk.sisby.switchy.Switchy",
			"client_init": "folk.sisby.switchy.client.SwitchyClient"
		},
		"depends": [
